{
  "name": "vue-simple-tips",
  "version": "0.1.6",
  "description":"vue简化版提示组件",
  "main": "lib/tips.umd.min.js",
  "keywords": ["vue","vue-tips","prompt"],
  "private": false,
  "author":"majiang",
  "license":"MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/majiang666/vue-simple-tips"
